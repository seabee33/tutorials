{
	"manifest_version": 3,
	"name":"Global Entry Drops",
	"description":"Find global entry interviews that meet your selction criteria",
	"version":"1.0",
	"action":{
		"default_popup": "popup/popup.html"
	},
	"icons":{
		"16":"icons/16.png",
		"32":"icons/32.png",
		"48":"icons/48.png",
		"128":"icons/128.png"
	},
	"background":{
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions":[
		"storage"
	]
}